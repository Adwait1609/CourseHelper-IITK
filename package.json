{
  "name": "course-helper",
  "version": "1.0.0",
  "description": "Course Helper Application",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd Backend && npm start",
    "start:frontend": "cd Front-End && npm start",
    "install:all": "npm install && cd Backend && npm install && cd ../Front-End && npm install",
    "build": "cd Front-End && npm run build",
    "test:api": "cd Backend && npm run test:api",
    "test:db": "cd Backend && npm run test:db"
  },
  "keywords": [
    "course",
    "helper",
    "education"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
